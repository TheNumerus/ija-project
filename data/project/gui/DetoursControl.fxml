<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<fx:root maxWidth="1.7976931348623157E308" spacing="10.0" type="VBox" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.gui.DetoursControl">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
   <children>
        <Button fx:id="SelectClosedSegments" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Select closed segment" onAction="#SelectClosedSegmentsButtonClick"/>
        <Button fx:id="SelectDetourSegments" disable="true" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Select detour segments" onAction="#SelectDetourSegmentsButtonClick"/>
        <Button fx:id="Apply" disable="true" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Apply" onAction="#ApplyButtonClick"/>
        <Button fx:id="RemoveDetour" disable="true" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Remove Detour" onAction="#RemoveButtonClick"/>
   </children>
</fx:root>
