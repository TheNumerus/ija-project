<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane prefHeight="360" prefWidth="800.0" styleClass="background" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.Controller">
   <columnConstraints>
      <ColumnConstraints hgrow="ALWAYS" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints vgrow="ALWAYS" />
      <RowConstraints />
   </rowConstraints>
   <GridPane styleClass="background" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS">
      <Pane fx:id="MapPane" onMouseDragged="#onMouseDragged" onMousePressed="#onMousePressed" onScroll="#onScroll" onMouseClicked="#onMouseClicked" GridPane.columnIndex="1" GridPane.hgrow="SOMETIMES" GridPane.vgrow="ALWAYS">
         <Group fx:id="MapTransform" />
      </Pane>
      <BorderPane maxWidth="1.7976931348623157E308" minWidth="-Infinity" styleClass="backgroundUI" GridPane.vgrow="ALWAYS">
         <bottom>
            <VBox alignment="TOP_CENTER" maxWidth="1.7976931348623157E308" minWidth="-Infinity" spacing="10.0">
               <Label fx:id="time" text="00:00:00">
                  <font>
                     <Font size="18.0" />
                  </font>
                  <VBox.margin>
                     <Insets bottom="20.0" />
                  </VBox.margin>
               </Label>
               <Label text="1.0x" fx:id="speed"/>
               <GridPane hgap="10.0" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" vgap="10.0" VBox.vgrow="ALWAYS">
                 <columnConstraints>
                   <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" />
                   <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" />
                     <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" />
                 </columnConstraints>
                 <rowConstraints>
                   <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
                   <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
                 </rowConstraints>
                  <children>
                     <Button fx:id="slowDown_button" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#slowDown" text="Slow Down" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS" />
                     <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#resetSpeed" text="Reset Speed" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS" />
                     <Button fx:id="speedUp_button" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#speedUp" text="Speed Up" GridPane.columnIndex="2" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS" />
                     <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Jump Back" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1" GridPane.vgrow="ALWAYS" disable="true"/>
                     <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Pause" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1" GridPane.vgrow="ALWAYS" onAction="#playPause" fx:id="playPause_button" disable="true"/>
                     <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Jump Forward" GridPane.columnIndex="2" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1" GridPane.vgrow="ALWAYS" disable="true"/>
                  </children>
               </GridPane>
               <BorderPane.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </BorderPane.margin>
            </VBox>
         </bottom></BorderPane>
      <columnConstraints>
         <ColumnConstraints />
         <ColumnConstraints />
      </columnConstraints>
      <rowConstraints>
         <RowConstraints />
      </rowConstraints>
   </GridPane>
   <BorderPane styleClass="statusbar" GridPane.rowIndex="1">
      <left>
         <HBox alignment="CENTER" nodeOrientation="LEFT_TO_RIGHT" spacing="10.0">
            <Button mnemonicParsing="false" onAction="#loadDataButtonClick" text="LOAD DATA" textAlignment="CENTER" />
         </HBox>
      </left>
      <right>
         <Button contentDisplay="CENTER" layoutX="86.0" layoutY="20.0" mnemonicParsing="false" onAction="#resetView" text="RESET VIEW" textAlignment="CENTER" HBox.hgrow="ALWAYS" />
      </right>
      <padding>
         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </padding>
   </BorderPane>
</GridPane>
