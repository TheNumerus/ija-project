<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>

<GridPane prefHeight="360" prefWidth="640" styleClass="background" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.Controller">
   <columnConstraints>
      <ColumnConstraints hgrow="ALWAYS" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints vgrow="ALWAYS" />
      <RowConstraints />
   </rowConstraints>
   <Pane fx:id="MapPane" onMouseDragged="#onMouseDragged" onMousePressed="#onMousePressed" onScroll="#onScroll">
      <Group fx:id="MapTransform">
         <Polyline fill="TRANSPARENT" points="395.0, 227.62564086914062, 514.0, 165.5, 355.0, 91.5, 275.5, 165.5, 162.5, 204.5, 64.0, 150.5, 130.0, 91.5" strokeLineCap="ROUND" strokeLineJoin="ROUND" />
      </Group>
   </Pane>
   <BorderPane styleClass="statusbar" GridPane.rowIndex="1">
      <left>
         <HBox alignment="CENTER" nodeOrientation="LEFT_TO_RIGHT" spacing="10.0">
            <Label fx:id="TextLabel" alignment="CENTER" contentDisplay="CENTER" text="DAVIDE!" textAlignment="CENTER" />
            <Button fx:id="MainButton" mnemonicParsing="false" onAction="#onButtonClick" text="KLIKNI" textAlignment="CENTER" />
         </HBox>
      </left>
      <right>
         <Button contentDisplay="CENTER" layoutX="86.0" layoutY="20.0" mnemonicParsing="false" onAction="#onButtonClick" text="RESET VIEW" textAlignment="CENTER" HBox.hgrow="ALWAYS" />
      </right>
      <padding>
         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </padding>
   </BorderPane>
</GridPane>
